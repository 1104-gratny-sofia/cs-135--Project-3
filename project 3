#include <stdio.h> 


//get ones digit
int ones(int a, int b)
{
b = a % 10;
return b;
}
//get tens digit
int tens(int a, int b)
{
b = (a/10) %10;
return b;
}
//get hundreds digit
int hundreds(int a, int b)
{
b = (a/100) % 10;
return b;
}

//print #0-9
void print0(int line) {
	if (line == 0) {
	printf (" 00 ");
	}
	else if (line == 1) {
	printf ("0  0");
	}
	else if (line == 2) {
	printf ("0  0");
	}
	else if (line == 3) {
	printf ("0  0");
	}
	else {
	printf (" 00 ");
	}
}

void print1(int line) {
	if (line == 0) {
	printf (" 11");
	}
	else if (line == 1) {
	printf ("1 1 ");
	}
	else if (line == 2) {
	printf ("  1");
	}
	else if (line == 3) {
	printf ("  1");
	}
	else {
	printf ("  1");
	}
}

void print2(int line) {
	if (line == 0) {
	printf (" 22");
	}
	else if (line == 1) {
	printf ("2  2");
	}
	else if (line == 2) {
	printf ("  2");
	}
	else if (line == 3) {
	printf (" 2");
	}
	else {
	printf ("2222");
	}
}

void print3(int line) {
	if (line == 0) {
	printf ("3333");
	}
	else if (line == 1) {
	printf ("   3");
	}
	else if (line == 2) {
	printf ("333");
	}
	else if (line == 3) {
	printf ("   3");
	}
	else {
	printf ("3333");
	}
}

void print4(int line) {
	if (line == 0) {
	printf ("4   4");
