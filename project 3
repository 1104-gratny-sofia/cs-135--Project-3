void print9(int line) {
	if (line == 0) {
	printf ("9999");
	}
	else if (line == 1) {
	printf ("9  9");
	}
	else if (line == 2) {
	printf ("9999");
	}
	else if (line == 3) {
	printf ("   9");
	}
	else {
	printf ("   9");
	}
}


//print digit
void printdigit(int digit, int line) {
	switch(digit) {
	case 0:
	print0(line);
	break;
	case 1:
	print0(line);
	break;
	case 2:
	print0(line);
	break;
	case 3:
	print0(line);
	break;
	case 4:
	print0(line);
	break;
}
//print age
void printage(int hundreds, int tens, int ones)
{
int line;
printage(hundreds,tens,ones); {
line=0;
printdigit(hundreds, line);
printdigit(tens, line);
printdigit(ones, line);
printf("\n");
line=1;
printdigit(hundreds, line);
printdigit(tens, line);
printdigit(ones, line);
printf("\n");
line=2;
printdigit(hundreds, line);
printdigit(tens, line);
printdigit(ones, line);
printf("\n");
line=3;
printdigit(hundreds, line);
printdigit(tens, line);
printdigit(ones, line);
printf("\n");
line=4;
printdigit(hundreds, line);
printdigit(tens, line);
printdigit(ones, line);
printf("\n");




// function
int main() {

	// variables
	int age, hundreds, tens, ones;
	
	// Prompt & input
	printf("Please enter your age: ");
	scanf("%d", &age);

	hundreds = hundreds(age, hundreds);
	tens = tens(age, tens);
	ones = (age, ones);

	printage(hundreds, tens, ones);
}
