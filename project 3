	else if (line == 1) {
	printf ("4   4");
	}
	else if (line == 2) {
	printf ("44444");
	}
	else if (line == 3) {
	printf ("    4");
	}
	else {
	printf ("    4");
	}
}

void print5(int line) {
	if (line == 0) {
	printf ("5555");
	}
	else if (line == 1) {
	printf ("5   ");
	}
	else if (line == 2) {
	printf ("5555");
	}
	else if (line == 3) {
	printf ("   5");
	}
	else {
